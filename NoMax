%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%%%%%%%%%%%%%%%%% 非极大值抑制 %%%%%%%%%%%%%%%%%%%%%%%%%%%%%

function J=NoMax(I)
[m, n]=size(I);
J=zeros(m,n);
for j=1:n
    for i=2:m-1
        if I(i,j)>=I(i-1,j) && I(i,j)>=I(i+1,j)
            J(i,j)=I(i,j);
        else J(i,j)=0; 
        end
    end
end
